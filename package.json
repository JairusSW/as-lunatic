{
  "name": "as-lunatic",
  "version": "0.0.1",
  "types": "assembly/index.d.ts",
  "description": "Bindings for lunatic imports",
  "main": "index.js",
  "scripts": {
    "test": "run-s asbuild:channels asbuild:process asbuild:tcp test:channels test:process test:tcp",
    "test:channels": "lunatic build/channels.wasm",
    "asbuild:channels": "asc test/channels.ts --target debug --exportTable --binaryFile build/channels.wasm --textFile build/channels.wat",
    "test:process": "lunatic build/process.wasm",
    "asbuild:process": "asc test/process.ts --target debug --exportTable --binaryFile build/process.wasm --textFile build/process.wat",
    "test:tcp": "lunatic build/tcp.wasm",
    "asbuild:tcp": "asc test/tcp.ts --target debug --exportTable --binaryFile build/tcp.wasm --textFile build/tcp.wat"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lunatic-solutions/as-lunatic.git"
  },
  "keywords": [
    "lunatic",
    "wasm",
    "wasi"
  ],
  "author": "Joshua Tenner <tenner.joshua@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/lunatic-solutions/as-lunatic/issues"
  },
  "homepage": "https://github.com/lunatic-solutions/as-lunatic#readme",
  "devDependencies": {
    "as-wasi": "^0.4.4",
    "assemblyscript": "^0.18.9",
    "assemblyscript-regex": "^1.4.1",
    "npm-run-all": "^4.1.5"
  },
  "dependencies": {
    "@assemblyscript/loader": "^0.18.9"
  }
}

{
  "name": "as-lunatic",
  "version": "0.0.1",
  "types": "assembly/index.ts",
  "description": "Bindings for lunatic imports",
  "main": "index.js",
  "scripts": {
    "test": "run-s asbuild:channels asbuild:process asbuild:tcp test:channels test:process test:tcp",
    "test:channels": "lunatic build/debug/channels.wasm",
    "asbuild:channels": "asb assembly/test/channels.ts --target debug --wat",
    "test:process": "lunatic build/debug/process.wasm",
    "asbuild:process": "asb assembly/test/process.ts --target debug --wat",
    "test:tcp": "lunatic build/debug/tcp.wasm",
    "asbuild:tcp": "asb assembly/test/tcp.ts --target debug --wat"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lunatic-solutions/as-lunatic.git"
  },
  "keywords": [
    "lunatic",
    "wasm",
    "wasi"
  ],
  "author": "Joshua Tenner <tenner.joshua@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/lunatic-solutions/as-lunatic/issues"
  },
  "homepage": "https://github.com/lunatic-solutions/as-lunatic#readme",
  "devDependencies": {
    "asbuild": "^0.0.11",
    "assemblyscript": "^0.18.10",
    "npm-run-all": "^4.1.5"
  }
}

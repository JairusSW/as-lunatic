{
  "name": "as-lunatic",
  "version": "0.0.1",
  "types": "assembly/index.d.ts",
  "description": "Bindings for lunatic imports",
  "main": "index.js",
  "scripts": {
    "test": "run-s asbuild:channel asbuild:thread asbuild:tcp test:channel test:thread test:tcp",
    "test:channel": "lunatic build/debug/channel.wasm",
    "asbuild:channel": "asb test/channel.ts --target debug --wat",
    "test:thread": "lunatic build/debug/thread.wasm",
    "asbuild:thread": "asb test/thread.ts --target debug --wat",
    "test:tcp": "lunatic build/debug/net.wasm",
    "asbuild:tcp": "asb test/net.ts --target debug --wat"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lunatic-solutions/as-lunatic.git"
  },
  "keywords": [
    "lunatic",
    "wasm",
    "wasi"
  ],
  "author": "Joshua Tenner <tenner.joshua@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/lunatic-solutions/as-lunatic/issues"
  },
  "homepage": "https://github.com/lunatic-solutions/as-lunatic#readme",
  "devDependencies": {
    "asbuild": "^0.0.11",
    "assemblyscript": "^0.18.12",
    "npm-run-all": "^4.1.5"
  }
}
